<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>OnlineShopping</display-name>
<display-name>Email</display-name>
<welcome-file-list>
<welcome-file>Contact.jsp</welcome-file>
</welcome-file-list>
<servlet>
<servlet-name>FileUploadHandler</servlet-name>
<servlet-class>mypack.FileUploadHandler</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>FileUploadHandler</servlet-name>
<url-pattern>/FileUploadHandler/*</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>MailApp</servlet-name>
<servlet-class>mypack.MailApp</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>MailApp</servlet-name>
<url-pattern>/mailApp</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>SendMail</servlet-name>
<servlet-class>mypack.SendMail</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>SendMail</servlet-name>
<url-pattern>/sendMail</url-pattern>
</servlet-mapping>
</web-app>